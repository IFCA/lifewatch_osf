VIRTUALENV="/home/ubuntu/.virtualenvs/lwdaap"
ENV_PYTHON="$VIRTUALENV/bin/python"

CELERYD_NODES="W1"
CELERY_BIN="$VIRTUALENV/bin/celery"
CELERY_APP="invenio.celery.celery"
CELERYD_CHDIR="/home/ubuntu/lwdaap"
CELERYD_OPTS="--purge -E"

CELERYD_USER=ubuntu
CELERYD_GROUP=ubuntu

CELERYD_LOG_FILE="/var/log/celery/%n.log"
CELERYD_PID_FILE="/var/run/celery/%n.pid"

CELERY_CREATE_DIRS=1
